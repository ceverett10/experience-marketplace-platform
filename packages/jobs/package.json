{
  "name": "@experience-marketplace/jobs",
  "version": "0.1.0",
  "private": true,
  "description": "Background job queue system for autonomous operations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@experience-marketplace/content-engine": "*",
    "@experience-marketplace/database": "*",
    "@experience-marketplace/holibob-api": "*",
    "@experience-marketplace/shared": "*",
    "@google-analytics/data": "^5.2.1",
    "@google-cloud/local-auth": "^3.0.1",
    "bullmq": "^5.1.0",
    "google-auth-library": "^10.5.0",
    "googleapis": "^171.0.0",
    "ioredis": "^5.3.0",
    "sharp": "^0.34.5",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/sharp": "^0.31.1",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
